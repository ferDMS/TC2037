#lang racket
(+ 2 4)
(display "hola mundo funcional")
(printf "Hola nuevamente soy yo, diosito")
(- 2 4)
(* 2 2)

; Estos operadores se pueden concatenar y así
(+ 2 3 4 5 6 6 7 8 9)
(/ 8 2 4 4)

; Utilizar una función dentro de otra
(+ 2 ( * 3 4))

; Una operación más compleja
(/ (+ 3 (/ (- 17 (* 7 4)) 8)) (/ (- 9 12) (- 1 (* 7 (+ -3 4)))) )

(sqr 3)
(sqrt 9)

; Trigonometría 
(sin pi)
(cos (/ pi 2))
(tan (/ pi 4))
(asin 0.999)
(acos pi)

; Strings y operaciones
(string-append "hello" "world")
(display (string-append "hello" "world"))

; Expresiones lógicas
(and #t #f)
(or #t #t)
(not #t)

; Definir una función
(define (mi_suma a b)
  (+ a b)
)